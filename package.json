{
  "name": "fallout4-auto-translator",
  "version": "1.0.1",
  "description": "auto translate MCN config text.",
  "main": "fallout4-auto-translator.exe",
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node16-linux-x64",
      "node16-win-x64"
    ]
  },
  "scripts": {
    "test": "tsc && node ./dist/index.js --modsDir playground\\mods --fileList playground\\mcmlist.txt --bilingual",
