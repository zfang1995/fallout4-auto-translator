    "test:translate-mcm": "node ./test/translate-mcm.js"
  },
  "author": "zfang1995",
  "license": "mozilla-public-license 2.0",
  "dependencies": {
    "@funny_town/pipeline.js": "^1.0.6",
    "chardet": "^1.4.0",
    "commander": "^9.4.0",
    "deepmerge": "^4.2.2",
    "fs-jetpack": "^4.3.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "p-limit": "^4.0.0",
    "promise-retry": "^2.0.1",
    "translate": "^1.4.1",
    "traverse": "^0.6.6"
  },
  "type": "module",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.0.6",
    "@types/promise-retry": "^1.1.3",
    "@types/traverse": "^0.6.32",
    "caxa": "^2.1.0"
  }
